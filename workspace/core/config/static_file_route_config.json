{
    "node_type": "namespace",
    "path" : "static",
    "route":{
        "js":{
            "node_type": "namespace",
            "path": "js",
            "route":{
                "jquery":{
                    "node_type": "javascript_file",
                    "content_type": "application/javascript",
                    "path":"jquery.js"
                }
            }
        },
        "page":{
            "node_type": "namespace",
            "path" : "html",
            "route":{
                "404":{"node_type":"page","content_type": "text/html","path": "404.html"}
            }
        }
    }
}
